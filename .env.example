# Copy this file to .env and set your settings.
# Database settings
DB_HOST=jp_api_mysql
DB_NAME=jsp_project
DB_USERNAME=root
DB_PASSWORD=root
# API Key Admin Settings
ADMIN_USERNAME=admin
ADMIN_PASSWORD=secret
# Email Settings
EMAIL_USE_SMTP=true
# Following fields are required for SMTP
EMAIL_HOST=host
EMAIL_USERNAME=user@email.com
EMAIL_PASSWORD=secret
EMAIL_PORT=465
# Cache Settings
USE_CACHE=false
CACHE_HOST='127.0.0.1'
CACHE_PORT=11211
# How long should we store cache?
CACHE_SECONDS=86400
# Google Analytics Settings
GA_TRACK_REQUESTS=false
GA_MEASUREMENT_ID=""
GA_SECRET=""
# Recaptcha Settings
RECAPTCHA_SECRET_KEY=""
RECAPTCHA_SITE_KEY=""
#  Are we running on a local machine?
IS_LOCAL=false
